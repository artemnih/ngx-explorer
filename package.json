{
  "name": "explorer-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ngx-explorer && npm run copy",
    "copy": "cp README.md dist/ngx-explorer/",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "semantic-release": "semantic-release"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~16.2.9",
    "@angular/common": "~16.2.9",
    "@angular/compiler": "~16.2.9",
    "@angular/core": "~16.2.9",
    "@angular/platform-browser": "~16.2.9",
    "@angular/platform-browser-dynamic": "~16.2.9",
    "@angular/router": "~16.2.9",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.2.6",
    "@angular/cli": "~16.2.6",
    "@angular/compiler-cli": "~16.2.9",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.1",
    "@types/jasmine": "~5.1.0",
    "@types/jasminewd2": "~2.0.11",
    "@types/node": "^18.0.0",
    "ng-packagr": "^16.2.3",
    "semantic-release": "^22.0.5",
    "ts-node": "~10.9.1",
    "typescript": "~5.0.0"
  },
  "release": {
    "branches": [
      "dev"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist/ngx-explorer"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "cp ./dist/ngx-explorer/package.json ./projects/ngx-explorer/package.json"
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "projects/ngx-explorer/package.json"
          ]
        }
      ]
    ]
  }
}